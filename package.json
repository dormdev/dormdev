{
  "name": "dormdev",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "develop": "next",
    "build": "next build",
    "start": "next start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{html,js,json,md,mdx,mjs,yml}": [
      "prettier --no-semi --single-quote --write"
    ],
    "*.gif": [
      "imageoptim"
    ],
    "*.jpg": [
      "imageoptim"
    ],
    "*.png": [
      "imageoptim --no-imageoptim --imagealpha"
    ]
  },
  "dependencies": {
    "@zeit/next-source-maps": "^0.0.3",
    "focus-visible": "^5.0.2",
    "next": "^10.0.2",
    "next-manifest": "^3.0.1",
    "next-offline": "^5.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.3",
    "react-items-carousel": "^2.8.0",
    "react-progressive-image": "^0.6.0",
    "react-tiny-popover": "^5.1.0",
    "react-typed": "^1.2.0",
    "styled-components": "^5.1.1"
  },
  "resolutions": {
    "react-items-carousel/styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@dormdev/eslint-config": "^0.1.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-styled-components": "^1.8.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "husky": "^4.2.5",
    "imageoptim-cli": "^3.0.2",
    "jest": "^24.9.0",
    "lint-staged": "^10.2.11",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.12.0"
  }
}
